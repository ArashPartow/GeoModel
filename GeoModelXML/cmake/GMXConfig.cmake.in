# Find the dependencies of the GMX target(s).
include( CMakeFindDependencyMacro )
find_dependency( GeoModelCore @GeoModelCore_VERSION@ CONFIG EXACT )
find_dependency( GeoModelTools @GeoModelTools_VERSION@ CONFIG EXACT )
find_dependency( XercesC @XercesC_VERSION@ )
find_dependency( ZLIB @ZLIB_VERSION@ )

# Include the exported configuration of GeoModelCore.
get_filename_component( SELF_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH )
include( ${SELF_DIR}/GMXTargets.cmake )

# Set the version of the installed project.
set( GMX_VERSION "@PROJECT_VERSION@" )

# Print some standard messages about the package being found.
include( FindPackageHandleStandardArgs )
find_package_handle_standard_args( GMX
    FOUND_VAR GMX_FOUND
    REQUIRED_VARS SELF_DIR
    VERSION_VAR GMX_VERSION )

# Clean up.
unset( SELF_DIR )
